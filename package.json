{
  "name": "naman-platform-new-age",
  "version": "1.0.0",
  "description": "a bundle with latest standards for react and tailwind",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build": "rm -rf ./dist && microbundle-crl"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint \"src/**/*.{js,jsx}\" --fix",
      "prettier --write \"src/**/*.{js,jsx}\""
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namanavasthi/naman-platform-new-age.git"
  },
  "keywords": [],
  "author": "Naman Avasthi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/namanavasthi/naman-platform-new-age/issues"
  },
  "homepage": "https://github.com/namanavasthi/naman-platform-new-age#readme",
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
